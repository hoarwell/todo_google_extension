{
  "name": "Todo Extension",
  "description": "Base Level Extension",
  "version": "1.0",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Todo"
  },
  "icons": {
    "16": "logo.png",
    "48": "logo.png",
    "128": "logo.png"
  },
  "background":{
    "scripts": ["background.js"]
  },
  "content_security_policy": "script-src 'self' 'sha256-iciaI/VOay42TkPam02XQWgcGc2mWCPFbqRPgfc4U94=' 'unsafe-eval'; object-src 'self'"
}
